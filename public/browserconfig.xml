<?xml version="1.0" encoding="utf-8"?>
<!--
  Browser Configuration for Rastasafari Experience Jamaica
  Used by Microsoft Edge and Internet Explorer for tile customization
-->
<browserconfig>
  <msapplication>
    <tile>
      <square70x70logo src="/mstile-70x70.png"/>
      <square150x150logo src="/mstile-150x150.png"/>
      <square310x310logo src="/mstile-310x310.png"/>
      <wide310x150logo src="/mstile-310x150.png"/>
      <TileColor>#009B3A</TileColor>
    </tile>
    <notification>
      <polling-uri src="https://rastasafari.com/notifications/feed.xml"/>
      <frequency>30</frequency>
      <cycle>1</cycle>
    </notification>
  </msapplication>
</browserconfig>
